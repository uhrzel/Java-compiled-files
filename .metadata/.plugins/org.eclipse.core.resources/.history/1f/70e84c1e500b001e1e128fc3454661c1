        JLabel lblNewLabel_5 = new JLabel("Number 4:");
        lblNewLabel_5.setBounds(20, 68, 80, 14);
        panel2.add(lblNewLabel_5);

        txtnum4 = new JTextField();
        txtnum4.setBounds(120, 65, 86, 20);
        panel2.add(txtnum4);
        txtnum4.setColumns(10);

        JButton btnNewButton_1 = new JButton("Calculate");
        btnNewButton_1.addActionListener(new ActionListener() {
            public void actionPerformed(ActionEvent e) {
                int num3 = Integer.parseInt(txtnum3.getText());
                int num4 = Integer.parseInt(txtnum4.getText());

                int total3 = num3 * num4;
                int total4 = num3 / num4;

                AbstractButton txttot3;
				txttot3.setText(String.valueOf(total3));
                AbstractButton txttot4;
				txttot4.setText(String.valueOf(total4));

                CardLayout cardLayout = (CardLayout) layeredPane.getLayout();
                cardLayout.next(layeredPane);
            }
        });
        btnNewButton_1.setBounds(95, 107, 89, 23);
        panel2.add(btnNewButton_1);

        JTextField txttot3 = new JTextField();
        txttot3.setBounds(120, 170, 86, 20);
        panel2.add(txttot3);
        txttot3.setColumns(10);

        JTextField txttot4 = new JTextField();
        txttot4.setBounds(120, 205, 86, 20);
        panel2.add(txttot4);
        txttot4.setColumns(10);

        JLabel lblNewLabel_6 = new JLabel("Total 3:");
        lblNewLabel_6.setBounds(20, 173, 46, 14);
        panel2.add(lblNewLabel_6);

        JLabel lblNewLabel_7 = new JLabel("Total 4:");
        lblNewLabel_7.setBounds(20, 208, 46, 14);
        panel2.add(lblNewLabel_7);
